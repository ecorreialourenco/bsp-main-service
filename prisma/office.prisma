model Office {
  id        Int       @id @default(autoincrement())
  name      String
  address   String
  city      String
  state     String
  country   String
  zipCode   String
  companyId Int
  createdAt DateTime  @default(now())
  updatedAt DateTime  @default(now()) @updatedAt
  deletedAt DateTime?

  company Company @relation(fields: [companyId], references: [id])

  productStock ProductStock[]
}
